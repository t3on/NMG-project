	Journal = {Discourse Processes},
	Number = {2},
	Pages = {131-155},
	Title = {Underspecification and Aspectual Coercion},
	Url = {http://www.tandfonline.com/doi/abs/10.1207/s15326950dp4202_3},
	Volume = {42},
	Year = {2006},
