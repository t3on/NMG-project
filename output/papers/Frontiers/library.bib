	Month = {Oct},
	Number = {26},
	Pages = {4403-11},
	Pmid = {10789433},
	Pst = {ppublish},
	Title = {Eye movement control in reading: accounting for initial fixation locations and refixations within the E-Z Reader model},
	Volume = {39},
	Year = {1999},
