	Keywords = {cortical oscillations; language; multisensory integration; semantic memory; visual word recognition},
	Month = {Oct},
	Number = {43},
	Pages = {14318-23},
	Pmid = {25339744},
	Pst = {ppublish},
	Title = {Oscillatory Neuronal Activity Reflects Lexical-Semantic Feature Integration within and across Sensory Modalities in Distributed Cortical Networks},
	Volume = {34},
