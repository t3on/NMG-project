	Keywords = {angular gyrus; combinatorial semantics; compositionality; conceptual combination; semantic integration; semantic memory},
	Month = {Feb},
	Number = {7},
	Pages = {3276-84},
	Pmid = {25698762},
	Pst = {ppublish},
	Title = {Converging evidence for the neuroanatomic basis of combinatorial semantics in the angular gyrus},
	Volume = {35},
	Year = {2015},
