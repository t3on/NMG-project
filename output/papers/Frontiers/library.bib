	Journal = {J Neurosci},
	Journal-Full = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {Computational modeling; MTL; episodic memory; fMRI; free recall},
	Month = {Feb},
	Number = {7},
	Pages = {2914-26},
	Pmid = {25698731},
	Pst = {ppublish},
	Title = {Neural activity in the medial temporal lobe reveals the fidelity of mental time travel},
	Volume = {35},
