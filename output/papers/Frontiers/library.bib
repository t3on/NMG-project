	Journal = {Lang Cogn Process},
	Journal-Full = {Language and cognitive processes},
	Keywords = {morphology, semantics, irregular verbs},
	Month = {Dec},
	Number = {10},
	Pages = {1411-1461},
	Pmc = {PMC2998337},
	Pmid = {21151703},
	Pst = {ppublish},
	Title = {Lexical Semantics and Irregular Inflection},
