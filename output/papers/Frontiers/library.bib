	Number = {12},
	Pages = {2695 - 2698},
	Title = {Commentary on Evans and Levinson, the myth of language universals: Language diversity, cognitive universality},
	Url = {http://www.sciencedirect.com/science/article/pii/S0024384110000999},
	Volume = {120},
	Year = {2010},
