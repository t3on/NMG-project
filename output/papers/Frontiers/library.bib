	Mesh = {Amygdala; Animals; Benzophenanthridines; Conditioning, Classical; Fear; Hippocampus; Humans; Long-Term Potentiation; Male; Memory; Protein Kinase C; Protein Kinase Inhibitors; Rats},
	Month = {Dec},
	Number = {12},
