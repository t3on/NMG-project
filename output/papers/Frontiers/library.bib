@article{Drews:1996,
	Abstract = { Auditory word recognition studies have demonstrated that the responses to a target word (e.g. ``counted'') can be facilitated when it is preceded by a morphologically related prime word (e.g. ``counting''). This ``morphological priming'' can occur even when primes and targets are separated by a number of intervening items. This paper describes the different priming procedures and experimental variables used for gaining access to the nature of the lexical representation of morphological information and summarises the experimental data collected across different languages. },
	Author = {Drews, Etta},
