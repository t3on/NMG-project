	Issn = {0749-596X},
	Journal = {Journal of Memory and Language},
	Keywords = {automatic priming, semantic relatedness, associative relatedness, priming},
	Number = {4},
	Pages = {440 - 458},
	Title = {Effects of Semantic and Associative Relatedness on Automatic Priming},
