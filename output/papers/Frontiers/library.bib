	Journal = {Language and Cognitive Processes},
	Keywords = {compounds, semantic transparency},
	Number = {1-2},
	Pages = {261-290},
	Title = {The role of semantic transparency in the processing of Finnish compound words},
	Url = {http://www.tandfonline.com/doi/abs/10.1080/01690960444000098},
	Volume = {20},
	Year = {2005},
