@inproceedings{Bornkessel-Schlesewsky:2010,
	Abstract = {Object-extracted relative clauses (ORCs) are more difficult to process than subject-extracted relative clauses (SRCs), an asymmetry that depends on many lexical and discourse,3,4 factors. Studies that have manipulated the animacy of the two critical noun phrases (NPs)1 2 have shown that ORCs containing an animate NP1 and an inanimate NP2 (1d) are more difficult to process than SRCs (1a, 1c), and that the ORC-SRC asymmetry is reduced when NP1 is inanimate and NP2 is animate (1b). These results have been explained in terms of garden-pathing, where ORCs are initially interpreted as SRCs and subsequent reanalysis is easier when NP2 is animate3,4, or in terms of semantic indeterminacy, where reading-time patterns are proposed to reflect a reader's predictions for how a particular sentence should most likely unfold5. Two eye-tracking experiments were conducted to determine whether the apparent effects of NP animacy on the ORC asymmetry reflect distinct processes of interpretation that operate at NP2 and NP1 in stimuli demonstrating the animacy effect.},
	Address = {New York University},
	Author = {Ina Bornkessel-Schlesewsky and Franziska Kretzschmar and Matthias Schlesewsky},
	Booktitle = {The 24th Annual Conference on Human Sentence Processing},
	Date-Added = {2012-01-25 00:02:37 +0000},
