	Mesh = {Auditory Cortex; Female; Humans; Magnetoencephalography; Male; Photic Stimulation; Speech Perception; Young Adult},
	Month = {Jan},
	Number = {4},
	Pages = {1417-26},
	Pmid = {23345218},
	Pst = {ppublish},
	Title = {Visual input enhances selective speech envelope tracking in auditory cortex at a "cocktail party"},
