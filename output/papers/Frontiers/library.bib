	Journal-Full = {Nature reviews. Neuroscience},
	Keywords = {semantic network, N400, ERPs},
	Mesh = {Brain Mapping; Cerebral Cortex; Evoked Potentials; Humans; Models, Biological; Neural Pathways; Semantics},
	Month = {Dec},
