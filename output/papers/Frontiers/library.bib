	Keywords = {morphology, psycholinguistics, phonology, lexicon, decomposition, irregular allomorphy, lexical access, magnetoencephalography, complex words, visual word recognition},
	Number = {1},
	Pages = {85 - 123},
	Title = {A single route, full decomposition model of morphological complexity: MEG evidence.},
