	Keywords = {N-back, fMRI},
	Mesh = {Brain; Diagnostic Imaging; Humans; Memory; Meta-Analysis as Topic; Psychomotor Performance},
	Month = {May},
	Number = {1},
	Pages = {46-59},
	Pmid = {15846822},
	Pst = {ppublish},
	Title = {N-back working memory paradigm: a meta-analysis of normative functional neuroimaging studies},
