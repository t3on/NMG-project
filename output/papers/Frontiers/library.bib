	Keywords = {methodology},
	Month = {Jul},
	Number = {1},
	Pages = {212-8},
	Pmid = {22743800},
	Pst = {ppublish},
	Title = {Systematic biases in early ERP and ERF components as a result of high-pass filtering},
	Volume = {209},
	Year = {2012},
