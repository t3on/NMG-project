	Journal = {Electroencephalography and Clinical Neurophysiology},
	Keywords = {EEG, eye movements, EOG, dipole sources, multiple sources},
	Number = {3},
	Pages = {229 - 241},
	Title = {A multiple source approach to the correction of eye artifacts},
