	Journal = {J Neurosci Methods},
	Journal-Full = {Journal of neuroscience methods},
	Mesh = {Adult; Artifacts; Brain; Computer Simulation; Electroencephalography; Event-Related Potentials, P300; Eye Movements; Female; Humans; Male; Muscles; Signal Processing, Computer-Assisted},
	Month = {Sep},
	Number = {1},
