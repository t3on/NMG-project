	Journal = {J Neurosci},
	Journal-Full = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {EEG; bifurcation index; decision weighting; lexical decision; neural oscillations; sensory selection},
	Month = {Feb},
	Number = {7},
	Pages = {3256-62},
	Pmid = {25698760},
	Pst = {ppublish},
	Title = {Alpha phase determines successful lexical decision in noise},
	Volume = {35},
